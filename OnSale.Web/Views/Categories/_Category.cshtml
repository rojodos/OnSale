@model OnSale.Web.Models.CategoryViewModel

<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>

<div class="form-group">
    <div class="fileupload fileupload-new" data-provides="fileupload">
        <div class="fileupload-preview thumbnail" style="width: 200px; height: 200px;"></div>
        <div>
            <span class="btn btn-default btn-file"><span class="fileupload-new">Selecccionar imagen</span>
            <span class="fileupload-exists">Cambiar</span><input type="file" name="file" /></span>
            <a href="#" class="btn fileupload-exists" data-dismiss="fileupload">Quitar</a>
        </div>
    </div>
</div>

